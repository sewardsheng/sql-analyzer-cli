{
  "name": "sql-analyzer-cli",
  "version": "1.0.0",
  "description": "SQL语句智能分析与扫描CLI工具",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "sql-analyzer": "./src/index.js"
  },
  "scripts": {
    "dev": "bun run src/index.js",
    "prod": "bun run dist/index.js",
    "build": "bun build ./src/index.js --outdir ./dist --target node",
    "deploy": "node scripts/deploy.js",
    "health": "bun run src/index.js health",
    "api": "bun run src/index.js api",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:deployment": "node test/test-deployment.js",
    "lint": "echo 'Linting not configured yet'",
    "pre-commit": "node scripts/pre-commit.js"
  },
  "keywords": [
    "sql",
    "analyzer",
    "cli",
    "security",
    "performance"
  ],
  "author": "盛威",
  "license": "MIT",
  "dependencies": {
    "@langchain/classic": "^1.0.2",
    "@langchain/community": "^1.0.2",
    "@langchain/core": "^1.0.3",
    "@langchain/openai": "^1.1.0",
    "@langchain/textsplitters": "^1.0.0",
    "best-effort-json-parser": "^1.2.1",
    "chalk": "^5.3.0",
    "cli-progress": "^3.12.0",
    "cli-table3": "^0.6.5",
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "hono": "^4.0.0",
    "ink": "^6.5.0",
    "ink-select-input": "^6.2.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "inquirer": "^9.2.11",
    "langchain": "^1.0.4",
    "lucide-react": "^0.553.0",
    "ora": "^7.0.1",
    "react": "^19.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.7",
    "bun-types": "latest",
    "jest": "^29.7.0"
  },
  "engines": {
    "bun": ">=1.0.0"
  }
}
